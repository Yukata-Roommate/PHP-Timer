{
    "name": "yukata-roommate/timer",
    "description": "Timer for PHP",
    "type": "library",
    "license": "MIT",
    "version": "1.0.1",
    "autoload": {
        "psr-4": {
            "YukataRm\\Timer\\": "src",
            "YukataRm\\Laravel\\Timer\\": "src\\Laravel"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "YukataRm\\Laravel\\Timer\\Provider\\FacadeServiceProvider"
            ],
            "aliases": {
                "Timer": "YukataRm\\Laravel\\Timer\\Facade\\Timer"
            }
        }
    },
    "authors": [
        {
            "name": "Yukata Roommate",
            "email": "contact@yukata-roommate.com"
        }
    ],
    "require": {
        "php": ">=8.2.0",
        "yukata-roommate/static-proxy": "^1.0.0"
    },
    "require-dev": {
        "laravel/laravel": "^11.0"
    },
    "minimum-stability": "dev"
}